import { out } from std.io;
import { typeof } from std;

fun main(argv: List[String]) {
	out.write("Converting 5 to String\n");
	let x = 5 -> String;
	out.write(x);
	out.write(" : ");
	out.write(String.new(typeof(x)));
	out.write("\n\n");

	out.write("Converting \"99\" to Integer\n");
	let y = "99" -> Integer;
	out.write(String.new(y));
	out.write(" : ");
	out.write(String.new(typeof(y)));
	out.write("\n");
}
