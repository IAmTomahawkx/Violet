import { print } from std;

fun main(argv: List[String]): Void {
	let x = 1 + 2;
	print("1 + 2 = ", String.new(x));

	let y = 5 - 2;
	print("5 - 2 = ", String.new(y));

	let z = 5 * 2;
	print("5 * 2 = ", String.new(z));

	let a: Integer = 10 / 2;
	print("10 / 2 = ", String.new(a));

	let b: Integer = 5 % 2;
	print("5 % 2 = ", String.new(b));
	return nil;
}
